{
  "name": "api-entity",
  "version": "1.1.0",
  "description": "Create service entities according to your API agnostic to the framework you are using",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm unbuild",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "prepack": "pnpm unbuild",
    "release": "pnpm lint && standard-version && git push --follow-tags && pnpm publish"
  },
  "keywords": [
    "axios",
    "fetch",
    "agnostic",
    "model",
    "api",
    "entity",
    "typescript"
  ],
  "author": {
    "name": "jhony-v",
    "url": "https://github.com/jhony-v"
  },
  "repository": {
    "url": "https://github.com/jhony-v/api-entity"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.3",
    "unbuild": "^1.0.1"
  }
}
