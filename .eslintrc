{
	"parser": "@typescript-eslint/parser",
	"env": {
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"plugins": ["prettier", "@typescript-eslint"],
	"rules": {
		"prettier/prettier": "error"
	}
}
